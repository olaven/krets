{
    "files": [
        "app.ts"
    ],
    "quiet": false,
    "debug": true,
    "extensions": [
        ".js",
        ".ts"
    ],
    "interval": 500,
    "watch": [
        "."
    ],
    "deno_args": [
        "--allow-net",
        "--allow-read",
        "--allow-env",
        "--allow-write"
    ],
    "execute": {
        ".ts": ["deno", "run"]
    },
    "fmt": false
}
