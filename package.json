{
  "name": "krets",
  "version": "0.0.2",
  "description": "Frictionless feedback",
  "repository": "https://github.com/olaven/krets.git",
  "author": "olaven <olav@sundfoer.com>",
  "license": "AFL-3.0",
  "scripts": {
    "dev": "yarn migrate up && NODE_ENV=development NODE_OPTIONS='--inspect' next",
    "build": "next build",
    "start": "yarn migrate up --no-check-order --no-reject-unauthorized && NODE_ENV='production' next start",
    "test": "yarn migrate up && NODE_ENV=test jest --runInBand ./tests --forceExit --detectOpenHandles",
    "test:watch": "yarn test --watch --only-failures",
    "dev:integration": "yarn dev --port 3052",
    "cy:open": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run",
    "test:integration": "start-server-and-test dev:integration 3052 cy:run",
    "migrate": "node-pg-migrate"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.13.0",
    "@emotion/core": "^10.0.28",
    "@rebass/forms": "^4.0.6",
    "@stitches/react": "^0.0.3-canary.4",
    "array-move": "^3.0.1",
    "babel-plugin-emotion": "^10.0.33",
    "cors": "^2.8.5",
    "emotion-theming": "^10.0.27",
    "jspdf": "^2.1.1",
    "nanoid": "^3.1.12",
    "next": "^9.5.5",
    "node-kall": "^1.0.71",
    "node-pg-migrate": "^5.2.0",
    "nodemailer": "^6.4.11",
    "pg": "^8.2.1",
    "postmark": "^2.6.0",
    "randomcolor": "^0.6.2",
    "react": "^16.13.1",
    "react-code-blocks": "^0.0.8",
    "react-dom": "^16.13.1",
    "react-localization": "^1.0.15",
    "react-multi-select-component": "^2.0.12",
    "react-qrcode-logo": "^2.2.1",
    "react-spinners": "^0.9.0",
    "rebass": "^4.0.7",
    "typescript": "^3.9.5",
    "victory": "^35.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.3",
